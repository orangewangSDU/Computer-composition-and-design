module ex3test_vlg_vec_tst();
// constants                                           
// general purpose registers
reg C_clk;
reg CLR;
reg Open;
reg S_clk;
// wires                                               
wire [7:0] output;
wire pin_name1;

wire sampler;                             

// assign statements (if any)                          
ex3test i1 (
// port map - connection between master ports and signals/registers   
	.C_clk(C_clk),
	.CLR(CLR),
	.Open(Open),
	.\output (output),
	.pin_name1(pin_name1),
	.S_clk(S_clk)
);

// Open
initial
begin
	Open = 1'b0;
	Open = #10000 1'b1;
end 

// CLR
initial
begin
	CLR = 1'b1;
end 

// S_clk
initial
begin
	S_clk = 1'b0;
	S_clk = #20000 1'b1;
end 

// C_clk
always
begin
	C_clk = 1'b0;
	C_clk = #500 1'b1;
	#500;
end 

ex3test_vlg_sample_tst tb_sample (
	.C_clk(C_clk),
	.CLR(CLR),
	.Open(Open),
	.S_clk(S_clk),
	.sampler_tx(sampler)
);

ex3test_vlg_check_tst tb_out(
	.output(output),
	.pin_name1(pin_name1),
	.sampler_rx(sampler)
);
endmodule


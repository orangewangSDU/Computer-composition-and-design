`timescale 1 ps/ 1 ps
module ex3test_vlg_sample_tst(
	C_clk,
	CLR,
	Open,
	S_clk,
	sampler_tx
);
input  C_clk;
input  CLR;
input  Open;
input  S_clk;
output sampler_tx;

reg sample;
time current_time;
always @(C_clk or CLR or Open or S_clk)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

